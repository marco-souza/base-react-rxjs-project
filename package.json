{
  "name": "base-react-rxjs-project",
  "version": "1.0.0",
  "description": "A simple bootstrap project to use React, Redux and RxJS to a full SPA.",
  "main": "dist/index.html",
  "repository": "git@github.com:marco-souza/base-react-rxjs-project.git",
  "author": "Marco Ant√¥nio <ma.souza.junior@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:dev": "NODE_ENV=development webpack-dev-server --open --config webpack.config.babel.js",
    "start:watch": "NODE_ENV=development webpack -f webpack.config.babel.js --progress --watch",
    "start": "yarn start:dev",
    "build:analyze": "BUNDLE_ANALYZE=true NODE_ENV=production webpack -f webpack.config.babel.js --progress",
    "build:prod": "NODE_ENV=production webpack -f webpack.config.babel.js --progress -p",
    "build:dev": "NODE_ENV=development webpack -f webpack.config.babel.js --progress",
    "build": "yarn build:prod",
    "update": "rm -rf node_modules yarn.lock; yarn"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "eslint": "^5.12.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "serviceworker-webpack-plugin": "^1.0.1",
    "terser-webpack-plugin": "^1.2.1",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rxjs": "^6.3.3"
  }
}
